<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doglovers.abandog.dao.DogDAOImg">
	
	

	<select id="selectDogList_cids" resultType="com.doglovers.abandog.dto.DogDTO">
		SELECT * FROM dog
		WHERE cid IN
		<foreach collection = 'array' index = 'index' item= 'cids_arr' open = "(" close = ')' separator = ','>
			#{cids_arr}
		</foreach> 
	</select>
	
</mapper>